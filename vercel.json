{
  "buildCommand": "flutter build web --web-renderer auto --release",
  "outputDirectory": "build/web",
  "installCommand": "flutter pub get",
  "framework": null,
  "devCommand": "flutter run -d web-server --web-port 3000",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "NEXT_PUBLIC_BLUEBUBBLES_HOST": "@bluebubbles_host",
    "NEXT_PUBLIC_BLUEBUBBLES_PASSWORD": "@bluebubbles_password"
  },
  "build": {
    "env": {
      "FLUTTER_VERSION": "3.24.0",
      "ENABLE_FLUTTER_WEB": "true"
    }
  }
}
